    // draw player name
    draw_text(xx, ycorner + yy * 1, "Player: " + string(cart_id));
    // angle
    draw_text(xx, ycorner + yy * 2, "Angle: " + string(cart_theta));
    
    // Health
    var hbx1 = xx;
    var hby1 = ycorner + yy * 3 + 5;
    var hbx2 = xx + 100;
    var hby2 = hby1 + yy + 5;
    draw_healthbar(hbx1, hby1, hbx2, hby2, cart_health, c_black, c_red, c_lime, 0, true, true)
}

var windtext = "-&gt;"
if (global.wind_angle == 180) {
    windtext = "&lt;-"
}
windtext = "Wind: " + windtext + ", " + string(global.wind_power);

draw_text(xcenter - (string_width(windtext) / 2), ycorner + lineheight, windtext);

