var lineheight = string_height("|");
var xcorner = 0;
var ycorner = 860;
var xcenter = room_width / 2;

with (obj_cart) {
    var xx = xcorner + 5; // cart 1 x
    var cart_theta = theta;
    var yy = lineheight;
    
    if (cart_id == 2) {
        xx = room_width - xx - string_width("player: 0000"); // cart 2
        cart_theta = 180 - theta;
